{
  "manifest_version": 2,
  "name": "Cute Save Button Ⲇ",
  "version": "23.9.25.1",
  "description": "Ⲇ is a Cute Save Button fork with additional features. Praise cute!",
  "default_locale": "en",
  "icons": {
    "16": "img/bestgirl-16.png",
    "32": "img/bestgirl-32.png",
    "48": "img/bestgirl-48.png",
    "64": "img/bestgirl-64.png",
    "96": "img/bestgirl-96.png",
    "128": "img/bestgirl-128.png",
    "176": "img/bestgirl-176.png"
  },
  "permissions": [
    "downloads",
    "storage",
    "*://*/*"
  ],
  "background": {
    "scripts": [
      "js/settings-default.js",
      "js/background.js",
      "js/download.js"
    ]
  },
  "content_scripts": [{
    "js": [
      "js/content-script.js"
    ],
    "matches": [
      "*://*/*"
    ],
    "run_at": "document_start",
    "all_frames": true
  }],
  "web_accessible_resources": [
    "img/bestgirl-96.png"
  ],
  "options_ui": {
    "open_in_tab": true,
    "page": "settings.html"
  },
  "browser_action": {
    "default_icon": {
      "16": "img/bestgirl-16.png",
      "32": "img/bestgirl-32.png",
      "48": "img/bestgirl-48.png",
      "64": "img/bestgirl-64.png",
      "96": "img/bestgirl-96.png",
      "128": "img/bestgirl-128.png",
      "176": "img/bestgirl-176.png"
    }
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "ochecuteextension@plaza.ink2",
      "strict_min_version": "79.0"
    }
  }
}